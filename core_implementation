account:
fields:
-username
-password
-isStudent
methods:
getters,setters
modifyAccount

teacher extends account:
methods:
addCourse
deleteCourse
editCourse
gradeQuiz
randomizeQuiz

student extends account:
methods:
takeQuizInTerminal
takeQuizViaFileUpload(1 int per line)
viewQuizResults

Course:
fields:
-name
-Arraylist<Quiz>
methods:
addQuizViaFileUpload(question prompt, options 1-4, correct answer choice(int))
addQuizInTerminal(Enter # of questions, enter the prompt, enter options 1-4, enter correct answer choice(int))
deleteQuiz
editQuiz

Quiz:
fields:
-Arraylist<Question>

Question:
fields:
-prompt
-Arraylist<String> answerChoices
-int correctAnswer

Manger:
methods:
addAccount
addCourse
deleteAccount
editAccount
readFile
writeFile
