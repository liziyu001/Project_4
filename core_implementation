account:
fields:
-username
-password
-isStudent
methods:
getters,setters
modifyAccount

teacher extends account:
methods:
addCourse
deleteCourse
editCourse
gradeQuiz
randomizeQuiz

student extends account:
methods:
takeQuizInTerminal
takeQuizViaFileUpload
viewQuizResults

Course:
fields:
-name
-Arraylist<Quiz>
methods:
addQuizViaFileUpload
addQuizInTerminal
deleteQuiz
editQuiz

Quiz:
fields:
-Arraylist<Question>

Question:
fields:
-prompt
-Arraylist<String> answers

Manger:
methods:
addAccount
addCourse
deleteAccount
editAccount
readFile
writeFile
